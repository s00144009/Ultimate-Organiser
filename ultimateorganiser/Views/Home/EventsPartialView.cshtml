@model IEnumerable<ultimateorganiser.Models.ClubEvent>
@using ultimateorganiser.Models


@if (Model != null)
{
    <div class="grid">
        <table cellspacing="0" width="80%">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.EventTitle)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EventDesc)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EventDate)
                    </th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td align="left">
                            @item.EventTitle
                        </td>
                        <td align="left">
                            @item.EventDesc
                        </td>
                        <td align="left">
                            @item.EventDate                                                  
                        </td>

                    </tr>
                }
            </tbody>
        </table>

    </div>

}
